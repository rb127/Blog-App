<%include ./partials/header %>

<h1>Welcome to the FrontPage</h1>
<ul>
  <% blogs.forEach((blog)=>{ %>
    <div>
      <h2><li><%= blog.title %></li></h2>
      <img src="<%= blog.image %>" alt="">
      <span><%= blog.created %></span>
      <p><i><%= blog.body %></i></p>
    </div>
<% }) %>
</ul>
<%include ./partials/footer %>
